{
  "name": "basic-platformer",
  "version": "0.1.0",
  "description": "A basic platformer game engine with Electron and some other technologies.",
  "main": "src_node/index.js",
  "scripts": {
    "start": "electron .",
    "makedo": "npm run build && npm start",
    "build": "npm run build:react -s",
    "build:react": "babel src_react -d src_compiled",
    "build:haskell": "hastec '--start=$$HASTE_MAIN();module.exports=Haste;' haskcode.hs",
    "postinstall": "npm run build",
    "watch": "babel -w src_react -d src_compiled"
  },
  "keywords": [],
  "author": "Logan Woodbury <Testare.i9z@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "electron": "^1.6.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "babel": {
    "presets": [
      "react",
      "es2017"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "electron-react-devtools": "^0.4.0"
  }
}
