{
  "name": "yage",
  "version": "0.6.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Testare/yage"
  },
  "description": "Yet Another Game Engine. A basic platformer game engine with Electron and some other technologies.",
  "main": "yage/launcher.js",
  "scripts": {
    "testart": "electron . 'stessa'",
    "test": "jest",
    "start": "electron . 'assets'",
    "makedo": "npm run build && npm start",
    "build": "npm run build:jsx -s && npm run build:docs -s",
    "build:jsx": "babel src/main/jsx -d yage --ignore '**/__tests__/*'",
    "build:docs": "cp -r src/main/docs/* yage",
    "postinstall": "npm run build",
    "watch": "babel -w src/main/jsx -d yage --ignore '**/__tests__/*'"
  },
  "files": [
    "build"
  ],
  "keywords": [],
  "author": "Logan Woodbury <Testare.i9z@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "electron": "^4.0.5",
    "html2canvas": "^1.0.0-alpha.12",
    "lodash": "^4.17.11",
    "npm": "^6.8.0",
    "react": "^15.5.4",
    "react-audio-player": "^0.11.0",
    "react-dom": "^15.5.4"
  },
  "babel": {
    "ignore": [
      "**/*.spec.jsx"
    ],
    "presets": [
      "@babel/react",
      "@babel/env"
    ],
    "plugins": [
      "macros",
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-macros": "^2.2.0",
    "electron-react-devtools": "^0.4.0",
    "jest": "^24.1.0"
  }
}
